@model List<Jeux>

<div class="catalogue">
	@{
		Layout = "~/Views/Shared/_LayoutSite.cshtml";
	}
	@{
		int max = (int)ViewData["i"] + 5;
		int depart = (int)ViewData["i"];
		List<Jeux> Jeux = Model;
		Jeux.Sort((a, b) => a.titre.CompareTo(b.titre));
	}
	<div classe="liste-jeux">
		@for (var i = depart; i < max; i++)
		{
			<div class="game">
				<p class="titre">@Jeux[i].titre</p>
				<img class="image" src="~/img/jeu/@Jeux[i].image" alt="Alternate Text" />
				<p>Description</p>
				<p class="description">@Jeux[i].description</p>
			</div>

			
		}
		@if(max<=)
		<button><a asp-controller="Home" asp-action="Catalogue" asp-route-i=@max>suite</a></button>

		
	</div>



