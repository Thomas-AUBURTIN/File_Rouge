<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApplication1</title>
    <link rel="stylesheet" href="~/css/normalize.css">
    <link rel="stylesheet" href="~/css/footer-header-background.css"  />
</head>
<body>
    <header>
        <img src="~/img/site/webclip-Photoroom.png" alt="logo" id="logo">
        <h1>LES MAUVAIS JOUEURS </h1>
        <div></div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2025 - WebApplication1 - <a asp-controller="Home" asp-action="Contact">Contact</a> - 
            <a  asp-controller="Home" asp-action="Privacy">Légal</a> - <a  asp-controller="Home" asp-action="Cookie">Cookie</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
